<p id="notice"><%= notice %></p>

<h1>Lab Queues</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lab_queues.each do |lab_queue| %>
      <tr>
        <td><%= lab_queue.name %></td>
        <td><%= lab_queue.date %></td>
        <% if (session[:current_user_vkid] == 140285287) %>
        <td><%= link_to 'Show', lab_queue %></td>
        <td><%= link_to 'Edit', edit_lab_queue_path(lab_queue) %></td>
        <td><%= link_to 'Destroy', lab_queue, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>

<h1>session["current_client_vkid"]</h1>
<%= session[:current_client_vkid] %>
<%= @vkid %>

<br>

<%= link_to 'New Lab Queue', new_lab_queue_path %>
